package com.javarush.task.jdk13.task04.task0438;

/* 
Рисуем линии
*/

public class Solution {
    public static void main(String[] args) {
        //напишите тут ваш код
       int i =0;
       int j=0;
       
           for (j=0; j<10;j++)
          { System.out.print("8");
       }
       System.out.println();
       for (i=0; i<10; i++)
       {System.out.println("8");
       }
    }
}
